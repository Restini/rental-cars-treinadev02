<h1>Registrar novo Fabricante</h1>

<% if @manufacturer.errors.any? %>
  <ul>
  <% if @manufacturer.errors.full_messages.each do |message| %>
  <li><%= message %></li>
  <% end %>
  <ul>
<% end %>

<%= form_with model: @manufacturer, local: true do |f| %>
<%= f.label :name, 'Nome' %>
<%= f.text_field :name %>
<%= f.submit 'Enviar' %>
<% end %>

<% end %>
